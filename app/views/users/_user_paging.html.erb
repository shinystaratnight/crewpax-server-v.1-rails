<script id="user_card_template" type="text/x-handlebars-template"> 
  <div class="pagination pagination-centered pager">
    <ul>
      {{#paginate pagination type="first"}}
        <li {{#if disabled}}class="disabled"{{/if}}><a href="/users">First</a></li>
      {{/paginate}}
      {{#paginate pagination type="previous"}}
        <li {{#if disabled}}class="disabled"{{/if}}><a href="?page={{n}}">Prev</a></li>
      {{/paginate}}
      {{#paginate pagination type="middle" limit="7"}}
        <li {{#if active}}class="active"{{/if}}><a href="?page={{n}}">{{n}}</a></li>
      {{/paginate}}
      {{#paginate pagination type="next"}}
        <li class= "next" {{#if disabled}}class="disabled"{{/if}}><a href="?page={{n}}">Next</a></li>
      {{/paginate}}
      {{#paginate pagination type="last"}}
        <li {{#if disabled}}class="disabled"{{/if}}><a href="?page={{n}}">Last</a></li>
      {{/paginate}}
    </ul>
  </div>
</script>    
