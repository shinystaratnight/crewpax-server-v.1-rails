<%= content_tag_for :article, @user do %>
  <header>
    <div class="page-header">
      <h1>
        <%= @user.name %>
      </h1>
    </div>
  </header>
  <div class="row">
    <div class="col-md-4">
      <%= render "personalinfo" %>  
    </div> 
    <div class="col-md-1"></div>
    <div class="col-md-7">
      <%= render "union" %>
      <%= render "certificate" %>
      <%= render 'contacts' %>
    </div>
  </div>
 
<% end %>